{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-plusplus": "off",
        "no-console": "off",
        "no-throw-literal": "off",
        "no-restricted-syntax": "off",
        "prefer-destructuring": "off"
    }
}
